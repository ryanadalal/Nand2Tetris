/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=b, b=sel,out=outb);
    Not(in=sel,out=nsel);
    And(a=a, b=nsel,out=outa);
    Or(a=outa,b=outb,out=out);
    //// Replace this comment with your code.
}